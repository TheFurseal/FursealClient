{
  "name": "furseal",
  "version": "1.0.0-alpha",
  "description": "The Furseal client using electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir ",
    "distMac": "electron-builder --mac --x64",
    "distWin": "electron-builder --win --x64"
  },
  "asar": false,
  "nodeIntegration": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheFurseal/FursealClient.git"
  },
  "keywords": [
    "Furseal"
  ],
  "author": "JohnSuu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheFurseal/FursealClient/issues"
  },
  "homepage": "https://github.com/TheFurseal/FursealClient#readme",
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "21.2.0",
    "electron-webrtc": "^0.3.0"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "defaults-deep": "^0.2.4",
    "diskusage-ng": "^0.2.4",
    "electron-updater": "^4.1.2",
    "ipfs": "^0.38.1",
    "ipfs-bitswap": "^0.25.1",
    "ipfs-repo": "^0.27.1",
    "level": "^5.0.1",
    "libp2p": "^0.26.2",
    "libp2p-bootstrap": "0.9.7",
    "libp2p-connection-manager": "^0.1.0",
    "libp2p-crypto": "^0.16.1",
    "libp2p-kad-dht": "0.15.2",
    "libp2p-mdns": "0.12.3",
    "libp2p-mplex": "^0.8.5",
    "libp2p-pnet": "^0.1.0",
    "libp2p-secio": "0.11.1",
    "libp2p-spdy": "^0.13.3",
    "libp2p-switch": "^0.43.0",
    "libp2p-tcp": "0.13.0",
    "libp2p-webrtc-star": "^0.16.1",
    "libp2p-websocket-star": "^0.10.2",
    "lz4": "^0.6.3",
    "lz4js": "^0.2.0",
    "lzfjs": "^1.0.1",
    "merge-options": "1.0.1",
    "multiaddr": "6.1.0",
    "node-ipc": "^9.1.1",
    "npm": "^6.11.3",
    "peer-book": "0.9.1",
    "peer-id": "0.12.2",
    "peer-info": "0.15.1",
    "pull-mplex": "0.1.2",
    "pull-pushable": "2.2.0",
    "pull-stream": "^3.6.14",
    "rage-edit": "^1.2.0",
    "recursive-readdir": "^2.2.2",
    "webrtc": "^1.14.1"
  },
  "build": {
    "appId": "furseal",
    "win": {
      "requestedExecutionLevel": "highestAvailable"
    },
    "mac": {},
    "publish": [
      {
        "provider": "github",
        "owner": "theFurseal",
        "repo": "FursealClient"
      }
    ]
  },
  "postinstall": "electron-builder install-app-deps"
}
